{
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": "./",
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "downlevelIteration": true,
    "experimentalDecorators": true,
    "moduleResolution": "node",
    "importHelpers": true,
    "target": "es2015",
    "module": "es2020",
    "lib": [
      "es2018",
      "dom"
    ],
    "paths": {
      "arq-mf-lib": [
        "dist/arq-mf-lib/arq-mf-lib",
        "dist/arq-mf-lib"
      ],
      "amf-lib": [
        "dist/amf-lib/amf-lib",
        "dist/amf-lib"
      ],
      "arq-mf-weblib": [
        "dist/arq-mf-weblib/arq-mf-weblib",
        "dist/arq-mf-weblib"
      ]
    }
  }
}